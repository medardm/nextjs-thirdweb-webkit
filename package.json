{
  "name": "next-third-webkit",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "src/database/schema.prisma",
    "seed": "ts-node src/database/seed.ts"
  },
  "engines": {
    "node": ">=18",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "scripts": "npm run",
    "setup": "sh init.sh",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "deploy": "next build && next export && npx thirdweb@latest upload out",
    "test": "jest --watch ./src/tests/jest",
    "####---TEMPLATE SCRIPTS---#####": "----",
    "make:template": "hygen generator new",
    "make:help": "hygen make help",
    "make:controller": "hygen make controller",
    "make:model": "hygen make model",
    "make:model:only": "hygen make model:only",
    "make:model:table": "hygen make model:table",
    "make:seeder": "hygen make model:seeder",
    "make:factory": "hygen make model:factory",
    "make:test": "hygen make test",
    "postmake:model": "npx prisma generate",
    "postmake:model:table": "npx prisma generate",
    "####---DATABASE/PRISMA SCRIPTS---#####": "----",
    "migrate:dev": "npx prisma migrate dev && dotenv -e .env.test -- prisma migrate dev",
    "migrate:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build"
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@prisma/client": "^5.6.0",
    "@thirdweb-dev/chains": "^0",
    "@thirdweb-dev/react": "^4",
    "@thirdweb-dev/sdk": "^4",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "ethers": "^5",
    "formidable": "^3.5.1",
    "http-status-codes": "^2.3.0",
    "hygen": "^6.2.11",
    "lodash": "^4.17.21",
    "next": "^13",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.2",
    "react-dom": "^18.2",
    "react-hook-form": "^7.43.0",
    "react-hot-toast": "^2.4.0",
    "superjson": "^2.2.1",
    "ts-jest": "^29.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.0",
    "@testing-library/user-event": "14.5.1",
    "@types/formidable": "^3.4.5",
    "@types/node": "^18.11.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8.29.0",
    "eslint-config-next": "^13",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
